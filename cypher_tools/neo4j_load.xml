<tool id="neo4j_load" name="Neo4jLoad" version="1.0.0">
  <description></description>
  <command>
    (time neo4j-shell -file $infile > $outfile) 2>&amp;1
  </command>
  <inputs>
    <param name="infile" type="data" label="Cypher"/>
  </inputs>
  <outputs>
    <data format="tabular" name="outfile" label="${tool.name} on ${on_string}"/>
  </outputs>
  <help>
  </help>
</tool>
